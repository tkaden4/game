{
  "name": "game",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build:base": "npx esbuild src/game/index.mts --bundle --outfile=dist/index.mjs",
    "build": "yarn build:base --minify && node scripts/assets.mjs",
    "watch:dev": "yarn build:base --watch",
    "dev": "concurrently --kill-others \"yarn watch:dev\" \"node server.mjs\" \"node scripts/assets.mjs --watch\""
  },
  "devDependencies": {
    "@types/howler": "^2.2.7",
    "@types/lodash": "^4.14.191",
    "@types/matter-js": "^0.18.2",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.12",
    "glob": "^9.3.2",
    "node-watch": "^0.7.3",
    "pixi.js": "^7.2.2",
    "typescript": "^5.1.0-dev.20230324"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "howler": "^2.2.3",
    "lodash": "^4.17.21",
    "matter-js": "^0.19.0"
  }
}
